{
  "name": "@geoportallux/lux-3dviewer-themesync",
  "version": "1.4.4",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "vcmplugin build",
    "build": "vcmplugin build",
    "bundle": "vcmplugin bundle",
    "start": "vcmplugin serve --appConfig app.config.json",
    "preview": "vcmplugin preview",
    "buildStagingApp": "vcmplugin buildStagingApp",
    "lint:js": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "format": "prettier --write --list-different . && npm run lint:js -- --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit",
    "ensure-types": "vcmplugin ensure-types"
  },
  "author": "Camptocamp",
  "license": "GPL-3.0",
  "keywords": [
    "vcmap",
    "plugin"
  ],
  "files": [
    "src/",
    "dist/",
    "plugin-assets/",
    "LICENSE.md",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": "dist/index.js",
    "./dist": "./dist/index.js"
  },
  "repository": {
    "url": "https://github.com/Geoportail-Luxembourg/3dviewer-themesync"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "plugin-assets"
  ],
  "prettier": "@vcsuite/eslint-config/prettier.js",
  "peerDependencies": {
    "@vcmap/core": "^6.2.2",
    "@vcmap/ui": "^6.2.1",
    "vue": "~3.4.38",
    "vuetify": "~3.7.14"
  },
  "dependencies": {
    "@zip.js/zip.js": "2.7.34"
  },
  "devDependencies": {
    "@vcmap/plugin-cli": "^4.1.2",
    "@vcsuite/eslint-config": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "jest-canvas-mock": "^2.5.2",
    "jsdom": "^26.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  },
  "mapVersion": "^6.1"
}
